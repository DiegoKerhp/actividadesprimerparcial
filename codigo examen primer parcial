def calculador_de_componentes():
  print("ingresa la cantidad de aluminio que deseas")
  aluminio = input()
  print("ahora ingresa el precio unitario del aluminio")
  precio_unitario = input()
  precio1 = int(aluminio)*int(precio_unitario)
  print(precio1)
  print("ahora ingresa la cantidad de conectores")
  conectores = input()
  print("ingresa el precio unitario de los conectores")
  precio_unitario_2 = input()
  precio2 = int(conectores)*int(precio_unitario_2)
  print(precio2)
  print("ahora ingresa la cantidad de cajas")
  cajas = input()
  print("ingresa el precio unitario de las cajas")
  precio_unitario_3 = input()
  precio3 = int(cajas)*int(precio_unitario_3)
  print(precio3)
  print("este es tu costo final con un descuento del 5% por ser una empresa")
  print((precio1 + precio2 + precio3)-(precio1 + precio2 + precio3)*(5/100))

  
  


calculador_de_componentes()
